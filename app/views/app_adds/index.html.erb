<p id="notice"><%= notice %></p>

<h1>App Adds</h1>

<table>
  <thead>
    <tr>
      <th>App</th>
      <th>Name</th>
      <th>Ad mob</th>
      <th>Fb</th>
      <th>Is shown</th>
      <th>Is fb</th>
      <th>Is ad mob</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_adds.each do |app_add| %>
      <tr>
        <td><%= app_add.app.name %></td>
        <td><%= app_add.name %></td>
        <td><%= app_add.ad_mob_id %></td>
        <td><%= app_add.fb_id %></td>
        <td><%= app_add.is_shown %></td>
        <td><%= app_add.is_fb %></td>
        <td><%= app_add.is_ad_mob %></td>
        <td><%= link_to 'Show', app_add %></td>
        <td><%= link_to 'Edit', edit_app_add_path(app_add) %></td>
        <td><%= link_to 'Destroy', app_add, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App Add', new_app_add_path %>
