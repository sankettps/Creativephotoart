<p id="notice"><%= notice %></p>

<h1>App Devices</h1>

<table>
  <thead>
    <tr>
      <th>Device</th>
      <th>App</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_devices.each do |app_device| %>
      <tr>
        <td><%= app_device.device %></td>
        <td><%= app_device.app %></td>
        <td><%= link_to 'Show', app_device %></td>
        <td><%= link_to 'Edit', edit_app_device_path(app_device) %></td>
        <td><%= link_to 'Destroy', app_device, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App Device', new_app_device_path %>
